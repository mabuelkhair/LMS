<p id="notice"><%= notice %></p>

<h1>Listing Create Courses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @create_courses.each do |create_course| %>
      <tr>
        <td><%= link_to 'Show', create_course %></td>
        <td><%= link_to 'Edit', edit_create_course_path(create_course) %></td>
        <td><%= link_to 'Destroy', create_course, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Create course', new_create_course_path %>
